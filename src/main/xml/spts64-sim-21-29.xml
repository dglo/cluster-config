<icecube configName="spts64-sim-21-29"
         remarks="SPTS64 with simulated DOMs on 21 and 29">

  <cluster name="spts64">

    <!-- Non-fully-qualified paths are relative to pdaq metaproject on expcont -->
    <logDirForSpade>/mnt/data/pdaqlocal</logDirForSpade>

    <defaultLogLevel>INFO</defaultLogLevel>

    <location name="ichub21" host="spts64-ichub21">
      <module name="StringHub" id="1021"/>
    </location>

    <location name="ichub29" host="spts64-ichub29">
      <module name="StringHub" id="1029"/>
    </location>

    <location name="2ndbuild" host="spts64-expcont">
      <module name="SecondaryBuilders"/>
    </location>

    <location name="evbuilder" host="spts64-evbuilder">
      <module name="eventBuilder"/>
    </location>

    <location name="iitrigger" host="spts64-iitrigger">
      <module name="inIceTrigger"/>
    </location>

    <location name="gtrigger" host="spts64-gtrigger">
      <module name="globalTrigger"/>
    </location>
  </cluster>
</icecube>
